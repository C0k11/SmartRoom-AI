# Copy this file to .env and fill in your API keys

# Anthropic Claude API (Required for room analysis)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Replicate API (Required for image generation)
REPLICATE_API_TOKEN=your_replicate_token_here

# OpenAI API (Optional - for DALL-E 3 image generation)
OPENAI_API_KEY=your_openai_api_key_here

# Google OAuth (Required for Google login)
# Get credentials from Google Cloud Console: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Database (Optional - defaults to SQLite)
# DATABASE_URL=postgresql://user:password@localhost:5432/roomdesign

# Redis (Optional - for caching)
# REDIS_URL=redis://localhost:6379/0
